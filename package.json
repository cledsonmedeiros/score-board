{
  "name": "score-board",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:socket": "node server/socket-server.mjs",
    "dev:all": "concurrently \"pnpm dev\" \"pnpm dev:socket\"",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "preview:socket": "SOCKET_PORT=3001 node server/socket-server.mjs",
    "preview:all": "concurrently \"pnpm preview\" \"pnpm preview:socket\"",
    "start": "node .output/server/index.mjs",
    "start:socket": "node server/socket-server.mjs",
    "start:all": "concurrently \"pnpm start\" \"pnpm start:socket\"",
    "postinstall": "nuxt prepare",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@nuxt/fonts": "0.12.1",
    "@nuxt/icon": "2.1.0",
    "@pinia/nuxt": "^0.11.3",
    "@unhead/vue": "^2.0.19",
    "nuxt": "^4.2.1",
    "pinia": "^3.0.4",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.3",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@types/express": "^5.0.5",
    "concurrently": "^9.2.1",
    "express": "^5.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-classnames": "^0.8.5",
    "prettier-plugin-merge": "^0.8.0",
    "prettier-plugin-multiline-arrays": "^4.0.3",
    "prettier-plugin-tailwindcss": "^0.7.1"
  }
}
